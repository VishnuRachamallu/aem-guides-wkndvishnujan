<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:granite="http://www.adobe.com/jcr/granite/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Title"
    sling:resourceType="cq/gui/components/authoring/dialog"
    extraClientlibs="[core.wcm.components.title.v2.editor]"
    helpPath="https://www.adobe.com/go/aem_cmp_title_v2"
    trackingFeature="core-components:title:v2">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/container">
        <items jcr:primaryType="nt:unstructured">
            <tabsss
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/tabs">
                <items jcr:primaryType="nt:unstructured">
                    <container
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Container"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <title
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldDescription="Leave empty to use the page title."
                                fieldLabel="Title"
                                name="./title1"/>
                            <types
                                granite:class="core-title-sizes"
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                fieldLabel="Type / Size"
                                name="./type">
                                <datasource
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="core/wcm/components/title/v1/datasource/allowedtypes"/>
                            </types>
                            <defaulttypes
                                granite:class="core-title-sizes-default"
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                fieldLabel="Type / Size"
                                name="./typesone">
                                <items jcr:primaryType="nt:unstructured">
                                    <h1
                                        jcr:primaryType="nt:unstructured"
                                        text="H1"
                                        value="h1"/>
                                    <h2
                                        jcr:primaryType="nt:unstructured"
                                        text="H2"
                                        value="h2"/>
                                    <h3
                                        jcr:primaryType="nt:unstructured"
                                        text="H3"
                                        value="h3"/>
                                    <h4
                                        jcr:primaryType="nt:unstructured"
                                        text="H4"
                                        value="h4"/>
                                    <h5
                                        jcr:primaryType="nt:unstructured"
                                        text="H5"
                                        value="h5"/>
                                    <h6
                                        jcr:primaryType="nt:unstructured"
                                        text="H6"
                                        value="h6"/>
                                </items>
                            </defaulttypes>
                            <checkBox
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                                fieldDescription="Leave empty to use checkbox"
                                fieldLabel="CheckBox"
                                name="./checkbox1"/>
                            <dropdown_x0020_select
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                fieldDescription="Leave empty to use the page title."
                                fieldLabel="Drop down"
                                name="./dropdown">
                                <items jcr:primaryType="nt:unstructured">
                                    <h1
                                        jcr:primaryType="nt:unstructured"
                                        text="H1"
                                        value="h1"/>
                                    <h2
                                        jcr:primaryType="nt:unstructured"
                                        text="H2"
                                        value="h2"/>
                                    <h3
                                        jcr:primaryType="nt:unstructured"
                                        text="H3"
                                        value="h3"/>
                                    <vishnu
                                        jcr:primaryType="nt:unstructured"
                                        text="VISHNU"
                                        value="vishnusmallcase"/>
                                    <sam
                                        jcr:primaryType="nt:unstructured"
                                        text="samuel"/>
                                </items>
                            </dropdown_x0020_select>
                            <dropdownlib
                                granite:class="cq-dialog-dropdown-showhide"
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                fieldLabel="Select Gender"
                                name="./dropcomp">
                                <granite:data
                                    jcr:primaryType="nt:unstructured"
                                    cq-dialog-dropdown-showhide-target=".showhidetargets"/>
                                <items jcr:primaryType="nt:unstructured">
                                    <male
                                        jcr:primaryType="nt:unstructured"
                                        text="MALE"
                                        value="male"/>
                                    <female
                                        jcr:primaryType="nt:unstructured"
                                        text="FEMALE"
                                        value="female"/>
                                </items>
                            </dropdownlib>
                            <targetm
                                granite:class="showhidetargets"
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                <granite:data
                                    jcr:primaryType="nt:unstructured"
                                    showhidetargetvalue="male"/>
                                <items jcr:primaryType="nt:unstructured">
                                    <malefield
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldLabel="Mr"
                                        name="./tmale"/>
                                </items>
                            </targetm>
                            <targetf
                                granite:class="showhidetargets"
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                <granite:data
                                    jcr:primaryType="nt:unstructured"
                                    showhidetargetvalue="female"/>
                                <items jcr:primaryType="nt:unstructured">
                                    <femalefield
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldLabel="Ms"
                                        name="./fmale"/>
                                </items>
                            </targetf>
                        </items>
                    </container>
                </items>
            </tabsss>
        </items>
    </content>
</jcr:root>
